    "require": {
        "php" : "^7.0|^8.0|^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "MP\\Base\\": "src"
        },
        "files": [
            "src/Helpers/Methods.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MP\\Base\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MP\\Base\\MasterPackageServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
